{
	"schemaVersion": 1,
	"id": "upc",
	"version": "${version}",
	"name": "UPC",
	"description": "Adds a block to convert between Tech Reborn and Modern Industrialization energy",
	"authors": [
		"fooooooooooooooo, GoldenKitten"
	],
	"contact": {
		"homepage": "https://github.com/fooooooooooooooo/upc#readme",
		"sources": "https://github.com/fooooooooooooooo/upc",
		"issues": "https://github.com/fooooooooooooooo/upc/issues"
	},
	"license": "MIT",
	"icon": "assets/upc/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"me.goldenkitten.upc.UPC"
		],
		"client": [
			"me.goldenkitten.upc.UPCFabricClient"
		]
	},
	"mixins": [
		"upc.mixins.json",
		{
			"config": "upc.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.4",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*",
		"team_reborn_energy": ">=2.2.0",
		"modern_industrialization": ">=1.3.2"
	},
	"suggests": {
		"another-mod": "*"
	},
	"custom": {
		"megane:modules": [
			"me.goldenkitten.upc.compat.megane.UPCMeganeModule"
		]
	}
}